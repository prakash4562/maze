   wb = xlsx_package.workbook
   wb.add_worksheet(name: "Users") do |sheet|
   sheet.add_row %w(Name Email Number Posts Comments Likes)

@users.each do |user|
        if user.posts.length >= 10
            sheet.add_row [user.name , user.email, user.number, user.posts.length, user.comments.length, user.likes.length]
        end
      end
end